{
  "name": "htcats",
  "version": "0.0.0",
  "description": "HTTP status codes as cats",
  "license": "MIT",
  "author": "Curtis Doty <Curtis@GreenKey.net> (https://github.com/dotysan)",
  "homepage": "https://github.com/dotysan/htcats#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotysan/htcats.git"
  },
  "bugs": {
    "url": "https://github.com/dotysan/htcats/issues"
  },
  "keywords": [
    "cats",
    "http",
    "lolcats"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --showConfig && tsc --listEmittedFiles --diagnostics",
    "hello": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public",
    "bump": "git stash && npm version patch && git stash pop",
    "push": "git push --follow-tags"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.9.3"
  }
}
